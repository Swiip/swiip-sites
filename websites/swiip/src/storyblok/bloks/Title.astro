---
import { storyblokEditable } from "@storyblok/astro"
import type { HTMLTag } from "astro/types"
import type { TitleStoryblok } from "../types"

export type Props = { blok: TitleStoryblok }

const { blok } = Astro.props

const Tag = `h${blok.level}` as HTMLTag
---

<Tag class={Tag} {...storyblokEditable(blok)}>{blok.text}</Tag>
